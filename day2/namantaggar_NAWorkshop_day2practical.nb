Cell[BoxData[
 RowBox[{
  RowBox[{"bisectionMethod", "[", 
   RowBox[{"func_", ",", " ", "var_", ",", " ", "l_List", ",", " ", 
    RowBox[{"n_", ":", " ", "10"}], ",", " ", 
    RowBox[{"eps_", ":", " ", "0.0000005"}]}], "]"}], ":=", 
  RowBox[{"Module", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "xmin", ",", " ", "xmax", ",", " ", "f", ",", " ", "fv", ",", " ", 
      "mid"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{"Catch", "[", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"xmin", " ", "=", " ", 
       RowBox[{"l", "[", 
        RowBox[{"[", "1", "]"}], "]"}]}], ";", " ", 
      RowBox[{"xmax", " ", "=", " ", 
       RowBox[{"l", "[", 
        RowBox[{"[", "2", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"f", "[", "fv_", "]"}], ":=", " ", 
       RowBox[{"func", "/.", 
        RowBox[{"var", "->", "fv"}]}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"For", "[", 
       RowBox[{
        RowBox[{"i", " ", "=", " ", "1"}], ",", " ", 
        RowBox[{"i", " ", "<=", " ", "n"}], ",", " ", 
        RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"mid", " ", "=", " ", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{"xmin", " ", "+", " ", "xmax"}], ")"}], "/", "2"}]}], ";",
          "\[IndentingNewLine]", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"f", "[", "mid", "]"}], " ", "==", " ", "0"}], ",", " ", 
           RowBox[{"Throw", "[", 
            RowBox[{"{", 
             RowBox[{"i", ",", " ", 
              RowBox[{"N", "[", 
               RowBox[{"mid", ",", " ", "10"}], "]"}], ",", " ", 
              RowBox[{"N", "[", 
               RowBox[{
                RowBox[{"f", "[", "mid", "]"}], ",", " ", "10"}], "]"}]}], 
             "}"}], "]"}], ",", "\[IndentingNewLine]", 
           RowBox[{"If", "[", 
            RowBox[{
             RowBox[{
              RowBox[{
               RowBox[{"f", "[", "xmin", "]"}], 
               RowBox[{"f", "[", "mid", "]"}]}], "<", "0"}], ",", " ", 
             RowBox[{"xmax", " ", "=", " ", "mid"}], ",", " ", 
             RowBox[{"xmin", " ", "=", " ", "mid"}]}], "]"}]}], 
          "\[IndentingNewLine]", "]"}]}]}], "\[IndentingNewLine]", "]"}], ";",
       "\[IndentingNewLine]", 
      RowBox[{"Throw", "[", 
       RowBox[{"{", 
        RowBox[{"n", ",", 
         RowBox[{"N", "[", 
          RowBox[{"mid", ",", " ", "10"}], "]"}], ",", " ", 
         RowBox[{"N", "[", 
          RowBox[{
           RowBox[{"f", "[", "mid", "]"}], ",", " ", "10"}], "]"}]}], "}"}], 
       "]"}]}], "\[IndentingNewLine]", "]"}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
