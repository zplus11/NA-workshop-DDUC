Cell[BoxData[
 RowBox[{
  RowBox[{"gDMethod", "[", 
   RowBox[{"func_", ",", " ", "var_", ",", " ", "init_", ",", " ", 
    RowBox[{"iterations_", ":", " ", "10"}], ",", 
    RowBox[{"learningRate_", ":", "0.1"}]}], "]"}], ":=", 
  RowBox[{"Module", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "f", ",", " ", "ivar", ",", " ", "grad", ",", "new", ",", " ", "old"}], 
     "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{"Catch", "[", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"old", " ", "=", " ", "init"}], ";", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"f", "[", "ivar_", "]"}], ":=", 
       RowBox[{"func", "/.", 
        RowBox[{"var", "->", "ivar"}]}]}], ";", "\[IndentingNewLine]", 
